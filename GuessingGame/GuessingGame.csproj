  <ItemGroup>
    <Resource Include="images\C1.jpg" />
    <Resource Include="images\C2.jpg" />
    <Resource Include="images\C3.jpg" />
    <Resource Include="images\C4.jpg" />
    <Resource Include="images\J1.jpg" />
    <Resource Include="images\J2.jpg" />
